
config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc@0/c000000.wifi+1'
	option band '2g'
	option channel '1'
	option htmode 'HE20'
	option cell_density '0'
	option log_level '4'
	list hostapd_options 'vendor_elements=DD080050F21102000200'
	option txpower '30'
	option ldpc '0'

config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc@0/c000000.wifi'
	option band '5g'
	option channel '36'
	option htmode 'HE80'
	option country 'NZ'
	option cell_density '0'
	option log_level '4'
	list hostapd_options 'vendor_elements=DD080050F21102000200'
	option ldpc '0'

config wifi-iface 'wifinet0'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Adults2.4G'
	option encryption 'psk-mixed'
	option network 'adults_lan'
	option key 'asdf1234'
	option ifname 'Adults2.4G'

config wifi-iface 'wifinet1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Kids2.4g'
	option encryption 'none'
	option network 'kids_lan'
	option ifname 'Kids2.4g'

config wifi-iface 'wifinet2'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Kids_5.0g'
	option encryption 'none'
	option network 'kids_lan'
	option ifname 'Kids_5.0g'

config wifi-iface 'wifinet3'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Adults_5.0g'
	option encryption 'psk-mixed'
	option ifname 'Adults_5.0g'
	option key 'asdf1234'
	option network 'adults_lan'
