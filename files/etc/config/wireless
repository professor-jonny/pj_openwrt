
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '1'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option htmode 'HT20'
	option country 'NZ'
	option cell_density '0'
	option txpower '30'
	option log_level '4'
	list hostapd_options 'vendor_elements=DD080050F21102000200'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Adults_2.5g'
	option encryption 'psk2'
	option key 'asdf1234'
	option network 'adults_lan'
	option ifname 'Adults_2.5G'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ieee80211k '1'
	option time_advertisement '2'
	option time_zone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option wnm_sleep_mode '1'
	option bss_transition '1'
	option proxy_arp '1'
	option nasid 'adult'
	option wnm_sleep_mode_no_keys '1'
	option ft_psk_generate_local '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option htmode 'VHT80'
	option country 'NZ'
	option cell_density '0'
	option log_level '4'
	option txpower '24'
	list hostapd_options 'vendor_elements=DD080050F21102000200'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Adults_5g'
	option encryption 'psk2'
	option key 'asdf1234'
	option network 'adults_lan'
	option ifname 'Adults_5g'
	option ieee80211r '1'
	option ft_over_ds '0'
	option ieee80211k '1'
	option time_advertisement '2'
	option time_zone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option wnm_sleep_mode '1'
	option wnm_sleep_mode_no_keys '1'
	option bss_transition '1'
	option nasid 'adult'
	option proxy_arp '1'
	option ft_psk_generate_local '1'

config wifi-iface 'wifinet2'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Kids_2.5g'
	option encryption 'none'
	option network 'kids_lan'
	option ieee80211k '1'
	option time_advertisement '2'
	option time_zone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option bss_transition '1'
	option ifname 'Kids_2.5g'
	option wnm_sleep_mode '1'
	option wnm_sleep_mode_no_keys '1'
	option proxy_arp '1'

config wifi-iface 'wifinet3'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Kids_5g'
	option encryption 'none'
	option network 'kids_lan'
	option ieee80211k '1'
	option time_advertisement '2'
	option time_zone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option bss_transition '1'
	option ifname 'Kids_5g'
	option wnm_sleep_mode '1'
