
config system
	option hostname 'OpenWrt'
	option timezone 'NZST-12NZDT,M9.5.0,M4.1.0/3'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option zonename 'Pacific/Auckland'
	option log_proto 'udp'

config timeserver 'ntp'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'

config gpio_switch 'w_disable'
	option name 'W_DISABLE WWAN POWER CTRL'
	option gpio_pin '547'
	option value '1'

config gpio_switch 'modem_reset'
	option name 'MODEM_RESET CTRL'
	option gpio_pin '541'
	option value '0'

config gpio_switch 'ext_antenna_en'
	option name 'EXT_ANTENNA_ENABLE CTRL'
	option gpio_pin '568'
	option value '0'

config led
	option sysfs 'blue:status'
	option trigger 'default-on'
	option name 'blue power'

config led
	option name 'red transmit'
	option sysfs 'red:status'
	option trigger 'netdev'
	option dev 'wan'
	list mode 'tx'

config led
	option name 'green recieve'
	option sysfs 'green:status'
	option trigger 'netdev'
	option dev 'wan'
	list mode 'rx'

