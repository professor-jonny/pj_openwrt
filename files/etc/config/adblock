
config adblock 'global'
	option adb_enabled '1'
	option adb_debug '0'
	option adb_dnsfilereset '0'
	option adb_mail '0'
	option adb_backup '1'
	option adb_maxqueue '4'
	option adb_fetchutil 'uclient-fetch'
	option adb_dnsdir '/tmp/kids_lan/dnsmasq.d'
	option adb_trigger 'wan'
	option adb_dns 'dnsmasq'
	option adb_dnsinstance '1'
	option adb_report '1'
	option adb_repiface 'br-kids_lan'
	option adb_represolve '1'
	option adb_forcedns '0'
	option adb_safesearch '1'
	list adb_eng_sources 'extensions/social/formats/domains.txt'
	list adb_stb_sources 'alternates/social/hosts'
	list adb_utc_sources 'social_networks'
	list adb_utc_sources 'vpn'
	list adb_utc_sources 'doh'
	list adb_utc_sources 'chat'
	list adb_safesearchlist 'google'
	list adb_safesearchlist 'bing'
	list adb_safesearchlist 'duckduckgo'
	list adb_safesearchlist 'yandex'
	list adb_safesearchlist 'youtube'
	list adb_safesearchlist 'pixabay'
	list adb_sources 'stevenblack'
	list adb_sources 'utcapitole'

